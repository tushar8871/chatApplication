[Unit]
Description=gunicorn daemon
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/chatApplication/fundoo
ExecStart=/home/ubuntu/chatApplication/venv/bin/gunicorn --workers 3 --bind unix:/home/ubuntu/chatApplication/fundoo/fundoo.sock fundoo.wsgi:application
[Install]
WantedBy=multi-user.target
